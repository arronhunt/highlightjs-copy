{
  "name": "highlightjs-copy",
  "version": "1.0.3",
  "description": "A simple, accessible highlightjs plugin to add a copy button to your codeblocks.",
  "main": "dist/highlightjs-copy.min.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepare": "uglifyjs index.js -o ./dist/highlightjs-copy.min.js && uglifycss ./styles/highlightjs-copy.css > ./dist/highlightjs-copy.min.css && exit",
    "build": "esbuild index.js --outfile=dist/highlightjs-copy.js --bundle --target=chrome58,firefox57,safari11,edge16 --format=iife --global-name=CopyButtonPlugin"
  },
  "author": "Arron Hunt <arronjhunt@gmail.com>",
  "homepage": "https://github.com/arronhunt/highlightjs-copy",
  "license": "ISC",
  "devDependencies": {
    "esbuild": "^0.16.10",
    "uglify-js": "^3.13.9",
    "uglifycss": "0.0.29"
  }
}
